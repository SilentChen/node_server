<root>

	<Endpoint_Info>
		<arg type="int8" name="endpoint_type"/>
		<arg type="int32" name="endpoint_id"/>
		<arg type="string" name="endpoint_name"/>
		<arg type="string" name="server_ip"/>
		<arg type="int16" name="server_port"/>
		<arg type="int8" name="protocol_type"/>
	</Endpoint_Info>
	
	<Node_Info>
		<arg type="int8" name="node_type"/>
		<arg type="int32" name="node_id"/>
		<arg type="string" name="node_name"/>
		<arg type="string" name="node_ip"/>
		<vector type="Endpoint_Info" name="server_list"/>
	</Node_Info>

	<Token_Info>
		<arg type="int32" name="cid"/>
		<arg type="string" name="token"/>
		<arg type="int32" name="token_time"/>
	</Token_Info>
	
	<Role_Info>
    	<arg type="int64" name="role_id"/>
    	<arg type="string" name="role_name"/>
    	<arg type="string" name="account"/>
    	<arg type="int8" name="level"/>
    	<arg type="int32" name="exp"/>
    	<arg type="int8" name="gender"/>
    	<arg type="int8" name="career"/>
	</Role_Info>

	<Item_Info>
		<arg type="int32" name="item_id"/>
		<arg type="int32" name="amount"/>
		<arg type="int8" name="bind_type"/>
	</Item_Info>
	
	<Mail_Detail>
		<arg type="bool" name="pickup"/>
		<arg type="int32" name="mail_id"/>
		<arg type="int32" name="send_time"/>
		<arg type="int32" name="sender_type"/>
		<arg type="int64" name="sender_id"/>
		<arg type="string" name="sender_name"/>
		<arg type="string" name="mail_title"/>
		<arg type="string" name="mail_content"/>
		<arg type="int32" name="copper"/>
		<arg type="int32" name="gold"/>
		<vector type="Item_Info" name="item_info"/>
	</Mail_Detail>

	<Guild_Member_Detail>
		<arg type="int64" name="role_id"/>
		<arg type="string" name="role_name"/>
		<arg type="int8" name="level"/>
	</Guild_Member_Detail>
	
	<Rank_Member_Detail>
		<arg type="int64" name="role_id"/>
		<arg type="string" name="role_name"/>
		<arg type="int32" name="value"/>
	</Rank_Member_Detail>
	
	<Public_Player_Info>
    	<arg type="int64" name="role_id"/>
    	<arg type="string" name="account"/>
    	<arg type="string" name="role_name"/>
    	<arg type="int8" name="level"/>
    	<arg type="int8" name="gender"/>
    	<arg type="int8" name="career"/>
    	<arg type="int64" name="guild_id"/>
		<arg type="string" name="guild_name"/>
	</Public_Player_Info>
	
	<Player_Data>
		<struct type="Game_Player_Info" name="player_info"/>
		<struct type="Mail_Info" name="mail_info"/>
		<struct type="Bag_Info" name="bag_info"/>
	</Player_Data>
	
</root>
