<root>

	<DB_Player_Data>
		<struct type="DB_Role_Info" name="role_info"/>
		<struct type="DB_Mail_Info" name="mail_info"/>
		<struct type="DB_Bag_Info" name="bag_info"/>
	</DB_Player_Data>
	
	<DB_Item_Info>
		<arg type="int32" name="item_id"/>
		<arg type="int32" name="amount"/>
		<arg type="int8" name="bind_type"/>
	</DB_Item_Info>
	
	<DB_Mail_Detail>
		<arg type="bool" name="pickup"/>
		<arg type="int32" name="mail_id"/>
		<arg type="int32" name="send_time"/>
		<arg type="int32" name="sender_type"/>
		<arg type="int64" name="sender_id"/>
		<arg type="string" name="sender_name"/>
		<arg type="string" name="mail_title"/>
		<arg type="string" name="mail_content"/>
		<arg type="int32" name="copper"/>
		<arg type="int32" name="gold"/>
		<vector type="DB_Item_Info" name="item_info"/>
	</DB_Mail_Detail>
	
	<DB_Guild_Member_Detail>
		<arg type="int64" name="role_id"/>
		<arg type="string" name="role_name"/>
		<arg type="uint8" name="level"/>
	</DB_Guild_Member_Detail>
	
	<DB_Rank_Member_Detail>
		<arg type="int64" name="role_id"/>
		<arg type="string" name="role_name"/>
		<arg type="int32" name="value"/>
	</DB_Rank_Member_Detail>
	
	<DB_Idx_Info>
		<head table_name="game.idx" index_name="type"/>
		<arg type="string" name="type"/>
		<arg type="int32" name="value"/>
	</DB_Idx_Info>
	
	<DB_Role_Info>
		<head table_name="game.role" index_name="role_id"/>
		<arg type="int64" name="role_id"/>
		<arg type="string" name="role_name"/>
		<arg type="string" name="account"/>
		<arg type="uint8" name="level"/>
		<arg type="int32" name="exp"/>
		<arg type="int8" name="gender"/>
		<arg type="int8" name="career"/>
		<arg type="int32" name="create_time"/>
		<arg type="int32" name="login_time"/>
		<arg type="int32" name="logout_time"/>
		<arg type="int64" name="guild_id"/>
		<arg type="string" name="guild_name"/>
	</DB_Role_Info>

	<DB_Bag_Info>
		<head table_name="game.bag" index_name="role_id"/>
		<arg type="int64" name="role_id"/>
		<arg type="int32" name="copper"/>
		<arg type="int32" name="gold"/>
		<map type="DB_Item_Info" key_type="int32" key_name="item_id" name="item_map"/>
	</DB_Bag_Info>
	
	<DB_Mail_Info>
		<head table_name="game.mail" index_name="role_id"/>
		<arg type="int64" name="role_id"/>
		<arg type="int32" name="total_count"/>
		<map type="DB_Mail_Detail" key_type="int32" key_name="mail_id" name="mail_map"/>
	</DB_Mail_Info>
	
	<DB_Guild_Info>
		<head table_name="game.guild" index_name="guild_id"/>
		<arg type="int64" name="guild_id"/>
		<arg type="string" name="guild_name"/>
		<arg type="int64" name="chief_id"/>
		<arg type="int32" name="create_time"/>
		<vector type="DB_Guild_Member_Detail" name="member_list"/>
	</DB_Guild_Info>

	<DB_Rank_Info>
		<head table_name="game.rank" index_name="rank_type"/>
		<arg type="int64" name="rank_type"/>
		<arg type="int32" name="min_value"/>
		<arg type="int64" name="min_role_id"/>
		<map type="DB_Rank_Member_Detail" key_type="int64" key_name="role_id" name="member_map"/>
	</DB_Rank_Info>

	<DB_Logout_Info>
		<head table_name="log.logout" index_name="role_id"/>
		<arg type="int64" name="role_id"/>
		<arg type="string" name="role_name"/>
		<arg type="string" name="account"/>
		<arg type="uint8" name="level"/>
		<arg type="int32" name="exp"/>
		<arg type="int8" name="gender"/>
		<arg type="int8" name="career"/>
		<arg type="int32" name="create_time"/>
		<arg type="int32" name="login_time"/>
		<arg type="int32" name="logout_time"/>
	</DB_Logout_Info>	
	
</root>
